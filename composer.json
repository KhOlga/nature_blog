{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "knplabs/knp-markdown-bundle": "^1.7",
        "nexylan/slack-bundle": "^2.0",
        "php-http/guzzle6-adapter": "^2.0",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^5.0",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.10",
        "symfony/polyfill-php72": "^1.10",
        "symfony/routing": "^4.2",
        "symfony/security": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/twig-bundle": "^4.0",
        "symfony/web-server-bundle": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "easycorp/easy-log-handler": "^1.0.2",
        "symfony/debug-bundle": "^3.3|^4.2",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^3.3|^4.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^3.3|^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C9MGPF06NPVM5H66GEAE1TE5",
            "allow-contrib": false
        }
    }
}
